{
    "name": "memaudio-www",
    "version": "1.2.1",
    "license": "MIT",
    "author": {
        "name": "Bamdad Sabbagh",
        "email": "hello@bamdadsabbagh.com",
        "url": "https://bamdadsabbagh.com/"
    },
    "scripts": {
        "build": "next build",
        "dev": "next dev",
        "export": "next export",
        "start": "next start",
        "cra:build": "yarn build:run",
        "cra:build:clean": "rm -rf build",
        "cra:build:dev": "yarn build:clean && build:run",
        "cra:build:run": "GENERATE_SOURCEMAP=false SKIP_PREFLIGHT_CHECK=true react-scripts build",
        "cra:eject": "react-scripts eject",
        "cra:lint": "yarn lint:es && yarn lint:style",
        "cra:lint:es": "eslint src-cra --ext .js,.jsx,.ts,.tsx --fix",
        "cra:lint:style": "stylelint 'src-cra/**/*.{js,jsx,ts,tsx}' --fix",
        "package:clean": "rimraf node_modules",
        "package:reinstall": "yarn package:clean && yarn",
        "package:sort": "npx sort-package-json",
        "cra:start": "BROWSER=none SKIP_PREFLIGHT_CHECK=true react-scripts start",
        "cra:test": "react-scripts test"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "dependencies": {
        "next": "^10.2.3",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-icons": "^4.2.0",
        "react-player": "^2.9.0",
        "react-redux": "^7.2.4",
        "react-router-dom": "^5.2.0",
        "react-spring": "^9.2.3",
        "recoil": "^0.3.1",
        "redux": "^4.1.0",
        "redux-logger": "^3.0.6",
        "styled-components": "^5.3.0"
    },
    "devDependencies": {
        "@bamdadsabbagh/eslint-config": "^1.9.12",
        "@bamdadsabbagh/stylelint-config": "^1.3.11",
        "babel-plugin-styled-components": "^1.12.0",
        "eslint-plugin-prettier": "^3.4.0",
        "react-scripts": "^4.0.3",
        "rimraf": "^3.0.2",
        "sass": "^1.34.1",
        "sass-reset": "^1.0.2"
    }
}
